{
  "private": true,
  "name": "react-markdown-tree",
  "version": "0.0.0",
  "description": "Markdown provider and component for React",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run build-lib && yarn run build-dist",
    "build-lib": "rm -rf lib && babel src -d lib && flow-copy-source src lib",
    "build-dist": "rm -rf dist && webpack src/index.js dist/ReactMarkdownTree.js && webpack -p src/index.js dist/ReactMarkdownTree.min.js",
    "prepublish": "yarn run build",
    "dev": "yarn link && babel src -d lib --watch & yarn watch \"flow-copy-source src lib\" src",
    "postinstall": "flow-typed install"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafrex/react-markdown-tree.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Rafael Pedicini <code@rafrex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafrex/react-markdown-tree/issues"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.0.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^7.0.0",
    "flow-bin": "^0.46.0",
    "flow-copy-source": "^1.1.0",
    "prettier": "^1.3.1",
    "watch": "^1.0.2",
    "webpack": "^2.5.0"
  },
  "dependencies": {
    "commonmark": "^0.27.0",
    "commonmark-react-renderer": "^4.3.2",
    "prop-types": "^15.5.10",
    "warning": "^3.0.0"
  }
}
